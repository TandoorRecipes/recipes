  /api/food/:
    get:
      operationId: apiFoodList
      description: logs request counts to redis cache total/per user/
      parameters:
      - in: query
        name: expired
        schema:
          type: boolean
        description: Filter by whether food has expired inventory entries
      - in: query
        name: expiring_soon
        schema:
          type: integer
        description: Filter by foods with inventory expiring within N days
      - in: query
        name: has_children
        schema:
          type: boolean
        description: Filter by whether food has child foods
      - in: query
        name: has_inventory
        schema:
          type: boolean
        description: Filter by whether food has inventory entries with amount > 0
      - in: query
        name: has_recipe
        schema:
          type: boolean
        description: Filter by whether food has a linked recipe
      - in: query
        name: has_substitute
        schema:
          type: boolean
        description: Filter by whether food has substitutes configured
      - in: query
        name: ignore_shopping
        schema:
          type: boolean
        description: Filter by ignore_shopping flag
      - in: query
        name: in_shopping_list
        schema:
          type: boolean
        description: Filter by whether food is currently in an unchecked shopping
          list entry
      - in: query
        name: inventory_location
        schema:
          type: integer
        description: Filter by inventory location ID (only entries with amount > 0)
      - in: query
        name: limit
        schema:
          type: string
        description: limit number of entries to return
      - in: query
        name: onhand
        schema:
          type: boolean
        description: Filter by on-hand status for current user and shared users
      - in: query
        name: ordering
        schema:
          type: string
        description: 'Order results by field. Allowed: name, -name, numrecipe, -numrecipe,
          numchild, -numchild,supermarket_category__name, -supermarket_category__name.
          Ignored when query is active.'
      - name: page
        required: false
        in: query
        description: A page number within the paginated result set.
        schema:
          type: integer
      - name: page_size
        required: false
        in: query
        description: Number of results to return per page.
        schema:
          type: integer
      - in: query
        name: query
        schema:
          type: string
        description: Search foods by name (fuzzy or contains depending on user search
          preferences)
      - in: query
        name: random
        schema:
          type: string
        description: randomly orders entries (only works together with limit)
      - in: query
        name: root
        schema:
          type: integer
        description: Return first level children of food with ID [int]. Integer 0
          will return root foods.
      - in: query
        name: root_tree
        schema:
          type: integer
        description: Return all items belonging to the tree of the given food id
      - in: query
        name: supermarket_category
        schema:
          type: integer
        description: Filter by supermarket category ID
      - in: query
        name: tree
        schema:
          type: integer
        description: Return all self and children of food with ID [int].
      - in: query
        name: updated_at
        schema:
          type: string
        description: if model has an updated_at timestamp, filter only models updated
          at or after datetime
      - in: query
        name: used_in_recipes
        schema:
          type: boolean
        description: Filter by whether food is used as an ingredient in any recipe
